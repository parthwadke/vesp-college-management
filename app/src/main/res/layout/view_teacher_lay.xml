<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/textView4"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView6"
        android:layout_marginStart="87dp"

        android:layout_marginEnd="76dp"
        android:layout_marginBottom="32dp"
        android:text="Emergency Contact"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintEnd_toStartOf="@+id/t_emergency_contact"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        android:textColor="@color/colorSpinner"/>

    <TextView
        android:id="@+id/t_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/t_emergency_contact"
        android:layout_marginEnd="112dp"
        android:layout_marginBottom="32dp"
        android:layout_toRightOf="@id/textView9"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/t_id"
        app:layout_constraintEnd_toEndOf="parent"
        android:textColor="@color/colorYellow"/>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView4"
        android:layout_marginStart="87dp"
        android:layout_marginEnd="99dp"
        android:layout_marginBottom="32dp"
        android:text="Address"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toStartOf="@+id/t_address"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        android:textColor="@color/colorSpinner"/>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_marginStart="87dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="52dp"
        android:text="Email"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toStartOf="@+id/t_email"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        android:textColor="@color/colorSpinner"/>

    <TextView
        android:id="@+id/t_contact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/t_email"
        android:layout_marginEnd="112dp"
        android:layout_marginBottom="36dp"
        android:layout_toRightOf="@+id/textView6"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/t_emergency_contact"
        app:layout_constraintEnd_toEndOf="parent"
        android:textColor="@color/colorYellow"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView3"
        android:layout_marginStart="87dp"

        android:layout_marginEnd="100dp"
        android:layout_marginBottom="36dp"
        android:text="Contact"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toStartOf="@+id/t_contact"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        android:textColor="@color/colorSpinner"/>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView9"
        android:layout_marginStart="87dp"

        android:layout_marginEnd="138dp"
        android:layout_marginBottom="264dp"
        android:text="ID"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/t_id"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        android:textColor="@color/colorSpinner"/>

    <TextView
        android:id="@+id/t_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/t_address"
        android:layout_marginEnd="112dp"
        android:layout_marginBottom="264dp"
        android:layout_toRightOf="@id/textView2"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:textColor="@color/colorYellow"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="87dp"
        android:layout_marginTop="75dp"
        android:layout_marginEnd="110dp"
        android:layout_marginBottom="40dp"
        android:text="Name:"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toStartOf="@+id/t_name"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.605"
        android:textColor="@color/colorSpinner"/>

    <TextView
        android:id="@+id/t_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="75dp"
        android:layout_marginEnd="112dp"
        android:layout_marginBottom="40dp"
        android:layout_toRightOf="@id/textView"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/t_email"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.601"
        android:textColor="@color/colorYellow"/>

    <TextView
        android:id="@+id/t_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_marginEnd="112dp"
        android:layout_marginBottom="52dp"
        android:layout_toRightOf="@+id/textView3"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/t_contact"
        app:layout_constraintEnd_toEndOf="parent"
        android:textColor="@color/colorYellow"/>

    <TextView
        android:id="@+id/t_emergency_contact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/t_contact"
        android:layout_marginEnd="112dp"
        android:layout_marginBottom="48dp"
        android:layout_toRightOf="@+id/textView4"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/t_address"
        app:layout_constraintEnd_toEndOf="parent"
        android:textColor="@color/colorYellow"/>

</androidx.constraintlayout.widget.ConstraintLayout>